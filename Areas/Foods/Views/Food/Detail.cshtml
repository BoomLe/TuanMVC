@model App.Models.FoodModel

<h1>@Model.Name</h1>

<div class="card w-50">
    <img src="~/Food/@(Model.Name).jpg" alt=""/>
    <div class="card-body">
        <div class="card-text">
            @Model.Description
        </div>
        <a class="card-link" asp-action="Index"></a>

    </div>

</div>